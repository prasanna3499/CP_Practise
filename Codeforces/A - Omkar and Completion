import java.util.*;
public class A_OmkarandCompletion
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		for(int z=0;z<t;z++)
		{
		    int x=sc.nextInt();
		    for(int i=0;i<x;i++)
		       System.out.print("1"+" ");
		    System.out.println();
		}
	}
}
